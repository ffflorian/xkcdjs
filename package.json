{
  "author": "Florian Keller <github@floriankeller.de>",
  "bin": {
    "xkcdjs": "dist/cli.js"
  },
  "dependencies": {
    "@types/node": "10.12.7",
    "axios": "0.18.0",
    "commander": "2.19.0"
  },
  "description": "An xkcd API client with a CLI.",
  "devDependencies": {
    "@types/jasmine": "3.3.2",
    "@types/nock": "9.3.0",
    "jasmine": "3.3.1",
    "nock": "10.0.4",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "touch": "3.1.0",
    "ts-node": "7.0.1",
    "typedoc": "0.13.0",
    "typescript": "3.2.2"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "api",
    "cli",
    "typescript",
    "xkcd"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@ffflorian/xkcdjs",
  "repository": "https://github.com/ffflorian/xkcdjs.git",
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc --out docs/ --mode modules --excludePrivate --excludeExternals --exclude \"**/*(cli|index|RequestService).ts\"",
    "clear": "rimraf dist",
    "clear:docs": "rimraf docs",
    "dist": "yarn clear && yarn build",
    "dist:docs": "yarn clear:docs && yarn build:docs && nodetouch docs/.nojekyll",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.ts\"",
    "test": "ts-node -P tsconfig.jasmine.json node_modules/.bin/jasmine"
  },
  "types": "dist/index.d.ts",
  "version": "1.3.5"
}
