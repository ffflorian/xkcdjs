{
  "author": "Florian Keller <github@floriankeller.de>",
  "bin": {
    "xkcdjs": "dist/cli.js"
  },
  "dependencies": {
    "@types/node": "10.9.2",
    "axios": "0.18.0",
    "commander": "2.17.1"
  },
  "description": "An xkcd API client with a CLI.",
  "devDependencies": {
    "jasmine": "3.2.0",
    "nock": "9.6.1",
    "prettier": "1.14.2",
    "rimraf": "2.6.2",
    "touch": "3.1.0",
    "typedoc": "0.12.0",
    "typescript": "3.0.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "api",
    "cli",
    "typescript",
    "xkcd"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@ffflorian/xkcdjs",
  "repository": "https://github.com/ffflorian/xkcdjs.git",
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc --out docs/ --mode modules --excludePrivate --excludeExternals --exclude \"**/*(cli|index|RequestService).ts\"",
    "clear": "rimraf dist",
    "clear:docs": "rimraf docs",
    "dist": "yarn clear && yarn build",
    "dist:docs": "yarn clear:docs && yarn build:docs && nodetouch docs/.nojekyll",
    "prettier": "prettier --write \"src/*.ts\" \"spec/*.js\"",
    "test": "yarn dist && jasmine"
  },
  "types": "dist/index.d.ts",
  "version": "1.3.3"
}
